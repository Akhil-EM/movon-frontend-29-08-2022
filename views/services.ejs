	<link type="text/css" rel="stylesheet" href="css/style.css">
	<link type="text/css" rel="stylesheet" href="css/responsive.css">

	<!-- Icon -->
	<link rel="icon" href="img/favicon.png" sizes="32x32" />
</head>
<body class="has-spotlight ashade-smooth-scroll">
    <!-- Header -->
	<header id="ashade-header">
        <div class="ashade-header-inner">
            <div class="ashade-logo-block">
                <a href="/" class="ashade-logo is-retina">
                	<img src="img/logo.png" alt="Ashade Logo" width="128" height="110">
                </a>
            </div>
            <div class="ashade-nav-block">
				 <!-- navigation ad ejs file -->
				 <%include common/navigation.ejs %>
            </div>
        </div>
    </header>

    <!-- Content -->
    <!-- <div class="ashade-page-title-wrap">
        <h1 class="ashade-page-title">
            <span>What I'm offering</span>
            My Services
        </h1>
    </div> -->

    <main class="ashade-content-wrap">
		<div class="ashade-content-scroll">
			<div class="ashade-content">
				<section class="ashade-section">
					<div class="ashade-row">
						<div class="ashade-col col-12">
							<p class="ashade-intro">I offer my clients a wide range of services in various directions. Someone thinks that a professional photographer should be focused on one type of photography, but for my practice I have gained enough experience to feel confident in several different directions.</p>
						</div>
					</div><!-- .ashade-row -->
				</section>
				
				<section class="ashade-section">
					<div class="ashade-row">
                        <div class="ashade-col col-12">
							<div id="ashade-service-card-grid" class="ashade-service-card-grid">
								
								
								<!-- .ashade-service-card -->
							</div><!-- .ashade-service-card-grid -->
                        </div><!-- .ashade-col -->
                    </div><!-- .ashade-row -->
				</section>
				
			
				
				
			</div><!-- .ashade-content -->
			
			<!-- Footer -->
			<%include common/footer.ejs %>
		</div><!-- .ashade-content-scroll -->
	</main>
    
    <div class="ashade-to-top-wrap ashade-back-wrap">
        <div class="ashade-back is-to-top">
            <span>Back to</span>
            <span>Top</span>
        </div>
    </div>

    <!-- Aside Bar -->
    <%include common/contact-sidebar.ejs %>
    <!-- UI Elements -->
    <div class="ashade-menu-overlay"></div>
    <div class="ashade-aside-overlay"></div>
    <div class="ashade-cursor is-inactive">
